<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane  prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Faktura.Controller">
<children>
    <VBox VBox.vgrow="ALWAYS" alignment="TOP_RIGHT" fillWidth="false" prefHeight="800" prefWidth="700.0" spacing="5.0" >

        <children>
            <TableView fx:id="tableView" prefHeight="600.0" prefWidth="700.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="150.0" AnchorPane.topAnchor="0.0">

            </TableView>
        </children>
    </VBox>
    <AnchorPane layoutX="20.0" prefHeight="600.0" prefWidth="200">
        <children>
            <Button  fx:id="btnPrintAll" alignment="TOP_LEFT" layoutX="700.0" layoutY="20.0" mnemonicParsing="false" onAction="#rechnungenAusgebenController" text="Rechnungen drucken" />
            <Button  fx:id="btnGetRow" alignment="TOP_LEFT" layoutX="700.0" layoutY="60.0" mnemonicParsing="false" onAction="#rechnungEinzeltAusgeben" prefHeight="25.0" prefWidth="129.0" text="Wert drucken" />
        </children>

    </AnchorPane>
</children>
</AnchorPane>

