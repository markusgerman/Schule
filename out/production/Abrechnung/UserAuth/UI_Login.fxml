<VBox fx:id="vBox" prefHeight="400.0" prefWidth="500" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserAuth.Controller">
   <AnchorPane prefHeight="400.0" prefWidth="478.0">
      <Text layoutX="75.0" layoutY="186.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Benutzername" />
      <TextField fx:id="username" layoutX="173.0" layoutY="169.0" promptText="Benutzername" />
      <Text layoutX="104.0" layoutY="240.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Passwort" />
      <PasswordField fx:id="password" layoutX="173.0" layoutY="223.0" promptText="Passwort" />
      <Button layoutX="269.0" layoutY="277.0" mnemonicParsing="false" onAction="#LoginController" text="Sign In" />
   </AnchorPane>
</VBox>
